<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>RebexTinyTlsProxy</AssemblyName>
    <TargetFramework>net40</TargetFramework>
    <OutputType>Exe</OutputType>
    <RootNamespace>Rebex.TinyTlsProxy</RootNamespace>
    <ApplicationIcon>Resources\TinyTlsProxy.ico</ApplicationIcon>
    <PlatformTarget>AnyCPU</PlatformTarget>
    <Version>1.4.0</Version>
    <Product>Rebex Tiny TLS Proxy</Product>
    <Company>Rebex CR, s.r.o.</Company>
    <Description>A minimalist TLS proxy server with TLS 1.2 and TLS 1.3 support. Suitable for legacy operating systems.</Description>
    <Copyright>Copyright (c) 2019-2024 Rebex CR, s.r.o.</Copyright>
    <PackageProjectUrl>https://www.rebex.net/tls-proxy/</PackageProjectUrl>
    <StartupObject />
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="Resources\TinyTlsProxy.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Rebex.Common" Version="7.0.8865" />
    <PackageReference Include="Rebex.Common.Validator" Version="7.0.8865" />
    <PackageReference Include="Rebex.Networking" Version="7.0.8865" />
    <PackageReference Include="Rebex.Tls" Version="7.0.8865" />
    <PackageReference Include="Rebex.Elliptic.Castle" Version="1.2.1" />
    <PackageReference Include="Rebex.Elliptic.Curve25519" Version="1.2.1" />
  </ItemGroup>
</Project>